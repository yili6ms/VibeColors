{
  "name": "vibecolors",
  "displayName": "VibeColors",
  "description": "Modern, colorful VS Code theme designed for programmers with comprehensive dark and light variants",
  "version": "1.0.3",
  "publisher": "AlexLi",
  "icon": "vc.png",
  "engines": {
    "vscode": "^1.104.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "keywords": [
    "theme",
    "dark",
    "light",
    "modern",
    "colorful",
    "programming",
    "syntax-highlighting",
    "vibe",
    "developer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yili6ms/VibeColors.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "vibeColors.refresh",
        "title": "Refresh Dynamic Theme",
        "category": "VibeColors"
      },
      {
        "command": "vibeColors.refreshDark",
        "title": "Generate New Dark Theme",
        "category": "VibeColors"
      },
      {
        "command": "vibeColors.refreshLight",
        "title": "Generate New Light Theme",
        "category": "VibeColors"
      },
      {
        "command": "vibeColors.switchVariant",
        "title": "Switch Dark/Light Variant",
        "category": "VibeColors"
      },
      {
        "command": "vibeColors.savePalette",
        "title": "Save Current Palette",
        "category": "VibeColors"
      },
      {
        "command": "vibeColors.loadPalette",
        "title": "Load Saved Palette",
        "category": "VibeColors"
      },
      {
        "command": "vibeColors.regenerateDynamicConfig",
        "title": "Regenerate Dynamic Configuration",
        "category": "VibeColors"
      }
    ],
    "configuration": {
      "title": "VibeColors",
      "properties": {
        "vibeColors.persistSeed": {
          "type": "boolean",
          "default": false,
          "description": "Persist the last used seed between sessions"
        },
        "vibeColors.lastSeed": {
          "type": "number",
          "default": 0,
          "description": "Last used seed for palette generation"
        },
        "vibeColors.savedPalettes": {
          "type": "array",
          "default": [],
          "description": "Saved color palettes"
        },
        "vibeColors.autoRefreshInterval": {
          "type": "number",
          "default": 0,
          "description": "Auto-refresh interval in minutes (0 = disabled)"
        }
      }
    },
    "themes": [
      {
        "label": "VibeColors Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/VibeColors-dark-theme.json"
      },
      {
        "label": "VibeColors High Contrast Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/VibeColors-high-contrast-dark-theme.json"
      },
      {
        "label": "VibeColors Neon Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/VibeColors-neon-dark-theme.json"
      },
      {
        "label": "VibeColors Soft Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/VibeColors-soft-dark-theme.json"
      },
      {
        "label": "VibeColors Retro Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/VibeColors-retro-dark-theme.json"
      },
      {
        "label": "VibeColors Light",
        "uiTheme": "vs",
        "path": "./themes/VibeColors-light-theme.json"
      },
      {
        "label": "VibeColors Minimal Light",
        "uiTheme": "vs",
        "path": "./themes/VibeColors-minimal-light-theme.json"
      },
      {
        "label": "VibeColors Pastel Light",
        "uiTheme": "vs",
        "path": "./themes/VibeColors-pastel-light-theme.json"
      },
      {
        "label": "VibeColors Corporate Light",
        "uiTheme": "vs",
        "path": "./themes/VibeColors-corporate-light-theme.json"
      },
      {
        "label": "VibeColors Dynamic Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/VibeColors-dynamic-dark-theme.json"
      },
      {
        "label": "VibeColors Dynamic Light",
        "uiTheme": "vs",
        "path": "./themes/VibeColors-dynamic-light-theme.json"
      },
      {
        "label": "VibeColors Dynamic Vivid Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/VibeColors-dynamic-vivid-dark-theme.json"
      },
      {
        "label": "VibeColors Dynamic Vivid Light",
        "uiTheme": "vs",
        "path": "./themes/VibeColors-dynamic-vivid-light-theme.json"
      },
      {
        "label": "VibeColors Dynamic Muted Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/VibeColors-dynamic-muted-dark-theme.json"
      },
      {
        "label": "VibeColors Dynamic Muted Light",
        "uiTheme": "vs",
        "path": "./themes/VibeColors-dynamic-muted-light-theme.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "echo 'No linting configured'",
    "test": "node ./out/tests/color-utils.test.js"
  },
  "devDependencies": {
    "@types/node": "^16.x",
    "@types/vscode": "^1.104.0",
    "typescript": "^4.9.4"
  }
}
