	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#b28077"
			}
		},
		{
			"name": "String",
			"scope": ["string"],
			"settings": {
				"foreground": "#5a9965"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric", "constant.language", "constant.character"],
			"settings": {
				"foreground": "#c98900"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#d05991"
			}
		},
		{
			"name": "Function",
			"scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function"],
			"settings": {
				"foreground": "#f36c4f"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable", "string variable", "meta.definition.variable"],
			"settings": {
				"foreground": "#6c3c31"
			}
		},
		{
			"name": "Class & Type",
			"scope": ["entity.name.type", "support.type", "entity.name.class"],
			"settings": {
				"foreground": "#3c8fa0"
			}
		},
		{
			"name": "Constant",
			"scope": ["constant", "variable.other.constant"],
			"settings": {
				"foreground": "#c98900"
			}
		},
		{
			"name": "Punctuation",
			"scope": ["punctuation", "meta.brace", "meta.delimiter"],
			"settings": {
				"foreground": "#9f6d5e"
			}
		},
		{
			"name": "Markup Heading",
			"scope": ["markup.heading", "entity.name.section"],
			"settings": {
				"foreground": "#f36c4f"
			}
		}
	]
}
