	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#547080"
			}
		},
		{
			"name": "String",
			"scope": ["string"],
			"settings": {
				"foreground": "#8ff0c1"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric", "constant.language", "constant.character"],
			"settings": {
				"foreground": "#ffd37d"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#ff9b6a"
			}
		},
		{
			"name": "Function",
			"scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function"],
			"settings": {
				"foreground": "#42c5ff"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable", "string variable", "meta.definition.variable"],
			"settings": {
				"foreground": "#d7f7ff"
			}
		},
		{
			"name": "Class & Type",
			"scope": ["entity.name.type", "support.type", "entity.name.class"],
			"settings": {
				"foreground": "#72e4ff"
			}
		},
		{
			"name": "Constant",
			"scope": ["constant", "variable.other.constant"],
			"settings": {
				"foreground": "#ffa56e"
			}
		},
		{
			"name": "Punctuation",
			"scope": ["punctuation", "meta.brace", "meta.delimiter"],
			"settings": {
				"foreground": "#7ca0b5"
			}
		},
		{
			"name": "Markup Heading",
			"scope": ["markup.heading", "entity.name.section"],
			"settings": {
				"foreground": "#6ac8ff"
			}
		}
	]
}
