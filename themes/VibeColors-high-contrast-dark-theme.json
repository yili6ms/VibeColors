	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#888888"
			}
		},
		{
			"name": "String",
			"scope": ["string"],
			"settings": {
				"foreground": "#00ff00"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric"],
			"settings": {
				"foreground": "#ffff00"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#ff00ff"
			}
		},
		{
			"name": "Function",
			"scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function"],
			"settings": {
				"foreground": "#00ffff"
			}
		},
		{
			"name": "Class",
			"scope": ["entity.name.class", "entity.name.type", "support.type", "support.class"],
			"settings": {
				"foreground": "#ff8800"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable"],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Tag",
			"scope": ["entity.name.tag"],
			"settings": {
				"foreground": "#ff0000"
			}
		},
		{
			"name": "Attribute",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#00ff88"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"background": "#ff0000",
				"foreground": "#ffffff"
			}
		}
	]
}