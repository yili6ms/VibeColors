	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#aaaaaa"
			}
		},
		{
			"name": "String",
			"scope": ["string"],
			"settings": {
				"foreground": "#99e6b3"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric", "constant.language", "constant.character"],
			"settings": {
				"foreground": "#ffcc99"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#d4a5d4"
			}
		},
		{
			"name": "Function",
			"scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function"],
			"settings": {
				"foreground": "#99c5ff"
			}
		},
		{
			"name": "Class",
			"scope": ["entity.name.class", "entity.name.type", "support.type", "support.class"],
			"settings": {
				"foreground": "#a888c2"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable"],
			"settings": {
				"foreground": "#5a5a5a"
			}
		},
		{
			"name": "Tag",
			"scope": ["entity.name.tag"],
			"settings": {
				"foreground": "#ff9bb3"
			}
		},
		{
			"name": "Attribute",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#99e6d6"
			}
		},
		{
			"name": "Operator",
			"scope": ["keyword.control", "punctuation", "keyword.operator"],
			"settings": {
				"foreground": "#7a7a7a"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"background": "#ff9bb3",
				"foreground": "#ffffff"
			}
		}
	]
}