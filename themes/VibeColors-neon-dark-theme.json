	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6e7681"
			}
		},
		{
			"name": "String",
			"scope": ["string"],
			"settings": {
				"foreground": "#39ff14"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric", "constant.language", "constant.character"],
			"settings": {
				"foreground": "#ff8c00"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#ff1493"
			}
		},
		{
			"name": "Function",
			"scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function"],
			"settings": {
				"foreground": "#00ffff"
			}
		},
		{
			"name": "Class",
			"scope": ["entity.name.class", "entity.name.type", "support.type", "support.class"],
			"settings": {
				"foreground": "#ff69b4"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable"],
			"settings": {
				"foreground": "#e6edf3"
			}
		},
		{
			"name": "Tag",
			"scope": ["entity.name.tag"],
			"settings": {
				"foreground": "#ff0080"
			}
		},
		{
			"name": "Attribute",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#00ff80"
			}
		},
		{
			"name": "Operator",
			"scope": ["keyword.control", "punctuation", "keyword.operator"],
			"settings": {
				"foreground": "#00ffff"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"background": "#ff0080",
				"foreground": "#ffffff"
			}
		}
	]
}