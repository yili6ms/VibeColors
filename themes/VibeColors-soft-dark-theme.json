	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#939094"
			}
		},
		{
			"name": "String",
			"scope": ["string"],
			"settings": {
				"foreground": "#a6e3a1"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric", "constant.language", "constant.character"],
			"settings": {
				"foreground": "#fab387"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#cba6f7"
			}
		},
		{
			"name": "Function",
			"scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function"],
			"settings": {
				"foreground": "#89b4fa"
			}
		},
		{
			"name": "Class",
			"scope": ["entity.name.class", "entity.name.type", "support.type", "support.class"],
			"settings": {
				"foreground": "#f9e2af"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable"],
			"settings": {
				"foreground": "#fcfcfa"
			}
		},
		{
			"name": "Tag",
			"scope": ["entity.name.tag"],
			"settings": {
				"foreground": "#f38ba8"
			}
		},
		{
			"name": "Attribute",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#94e2d5"
			}
		},
		{
			"name": "Operator",
			"scope": ["keyword.control", "punctuation", "keyword.operator"],
			"settings": {
				"foreground": "#c1a78e"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"background": "#f38ba8",
				"foreground": "#221f22"
			}
		}
	]
}