	"tokenColors": [
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#95a5a6"
			}
		},
		{
			"name": "String",
			"scope": ["string"],
			"settings": {
				"foreground": "#27ae60"
			}
		},
		{
			"name": "Number",
			"scope": ["constant.numeric", "constant.language", "constant.character"],
			"settings": {
				"foreground": "#e67e22"
			}
		},
		{
			"name": "Keyword",
			"scope": ["keyword", "storage.type", "storage.modifier"],
			"settings": {
				"foreground": "#8e44ad"
			}
		},
		{
			"name": "Function",
			"scope": ["entity.name.function", "meta.function-call", "variable.function", "support.function"],
			"settings": {
				"foreground": "#3498db"
			}
		},
		{
			"name": "Class",
			"scope": ["entity.name.class", "entity.name.type", "support.type", "support.class"],
			"settings": {
				"foreground": "#9b59b6"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable"],
			"settings": {
				"foreground": "#2c3e50"
			}
		},
		{
			"name": "Tag",
			"scope": ["entity.name.tag"],
			"settings": {
				"foreground": "#e74c3c"
			}
		},
		{
			"name": "Attribute",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#1abc9c"
			}
		},
		{
			"name": "Operator",
			"scope": ["keyword.control", "punctuation", "keyword.operator"],
			"settings": {
				"foreground": "#34495e"
			}
		},
		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": {
				"background": "#e74c3c",
				"foreground": "#ffffff"
			}
		}
	]
}